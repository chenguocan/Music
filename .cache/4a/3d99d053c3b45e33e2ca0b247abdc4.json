r\n    height: 100px;\r\n    text-align: center;\r\n    .balls {\r\n      margin-top: 5px;\r\n      display: flex;\r\n      justify-content: center;\r\n      align-items: center;\r\n\r\n      span {\r\n        display: block;\r\n        width: 6px;\r\n        height: 6px;\r\n        border-radius: 50%;\r\n        background: $color2-darker;\r\n        margin-left: 5px;\r\n\r\n        &.current {\r\n          width: 8px;\r\n          height: 8px;\r\n          background: $color2;\r\n        }\r\n      }\r\n    }\r\n    h1{\r\n      color:$color;\r\n      font-size:20px;\r\n      padding-top: 20px;\r\n    }\r\n    p{\r\n      color:$color1;\r\n      font-size:12px;\r\n    }\r\n  }\r\n  .panels {\r\n    .panel-lyrics{\r\n      width: 100vw;\r\n      height:100%;\r\n      color:$color1;\r\n      text-align: center;\r\n      line-height: 2;\r\n      overflow: hidden;\r\n      .container {\r\n        transition: all .3s;\r\n        p {\r\n          font-size: 16px;\r\n          &.current {\r\n            color: $color;\r\n          }\r\n        }\r\n      }\r\n    }\r\n    height: calc(100vh - 240px);\r\n    width: 200vw;\r\n    display: flex;\r\n    align-items: center;\r\n    transition:all 1s;\r\n    &.panel1{\r\n      transform: translateX(0vw);\r\n    }\r\n    &.panel2{\r\n      transform: translateX(-100vw);\r\n    }\r\n    .area-effect {\r\n      .panel-effect {\r\n        width: 100vw;\r\n        .effect {\r\n          position: relative;\r\n          display: flex;\r\n          justify-content: center;\r\n          align-items: center;\r\n          height: 40vh;\r\n          > div {\r\n            background-size: contain;\r\n            position: absolute;\r\n          }\r\n        }\r\n      }\r\n\r\n      .effect-1 {\r\n        animation: rotate 20s linear infinite;\r\n        background: url(\"../svg/effect-no-move.svg\") 0 0 no-repeat;\r\n        width: 70vw;\r\n        height: 70vw;\r\n      }\r\n\r\n      .effect-2 {\r\n        background: url(\"../svg/effect-move1.svg\") 0 0 no-repeat;\r\n        width: 60vw;\r\n        height: 60vw;\r\n        animation: rotate 20s linear infinite reverse;\r\n      }\r\n\r\n      .effect-3 {\r\n        background: url(\"../svg/effect-move2.svg\") 0 0 no-repeat;\r\n        width: 24vw;\r\n        height: 24vw;\r\n        animation: rotate 20s linear infinite;\r\n      }\r\n\r\n      .lyrics {\r\n        text-align: center;\r\n        p {\r\n          font-size: 13px;\r\n          color: $color1;\r\n          margin-top: 3px;\r\n\r\n          &.current {\r\n            color: $color;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n  .footer {\r\n    height: 140px;\r\n    .buttons {\r\n      display: flex;\r\n      justify-content: center;\r\n\r\n      svg {\r\n        width: 24px;\r\n        height: 24px;\r\n        margin: 10px 20px;\r\n\r\n        &.btn-more {\r\n          transform: rotate(90deg);\r\n        }\r\n      }\r\n    }\r\n\r\n    .area-bar {\r\n      color: $color1;\r\n      font-size: 12px;\r\n      display: flex;\r\n      align-items: center;\r\n      padding: 0 20px;\r\n\r\n      .time-start {\r\n        order: 1;\r\n      }\r\n\r\n      .time-end {\r\n        order: 3;\r\n      }\r\n\r\n      .bar {\r\n        order: 2;\r\n        flex: 1;\r\n        height: 4px;\r\n        background: $color3;\r\n        border: 2px;\r\n        margin: 0 20px;\r\n\r\n        .progress {\r\n          width: 0%;\r\n          height: 100%;\r\n          border-radius: 2px;\r\n          background: $color2;\r\n          position: relative;\r\n\r\n          &::after {\r\n            content: \"\";\r\n            width: 16px;\r\n            height: 16px;\r\n            position: absolute;\r\n            right: 0;\r\n            top: -7px;\r\n            display: block;\r\n            background: url(\"../svg/progress.svg\") 0 0 no-repeat;\r\n            background-size: contain;\r\n          }\r\n        }\r\n      }\r\n    }\r\n\r\n    .actions {\r\n      display: flex;\r\n      align-items: center;\r\n      margin-top: 15px;\r\n      padding: 0 20px;\r\n      justify-content: space-between;\r\n\r\n      svg {\r\n        width: 28px;\r\n        height: 28px;\r\n\r\n        &.btn-play-pause {\r\n          width: 50px;\r\n          height: 50px;\r\n        }\r\n      }\r\n\r\n    }\r\n  }\r\n}\r\n@keyframes rotate{\r\n    0%{\r\n      transform: rotate(0);\r\n    }\r\n    100%{\r\n      transform: rotate(360deg);\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"4784d60db068ee3c7094c8f394567d2c","cacheData":{"env":{}}}